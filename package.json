{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/MigasEu/github-node.git"
  },
  "main": "build/server.js",
  "dependencies": {
    "express": "^4.16.4",
    "typed-rest-client": "^1.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/nock": "^9.3.1",
    "@types/node": "^11.13.5",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "nodemon": "^1.18.11",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.4"
  },
  "scripts": {
    "dev": "node --inspect=${NODE_INSPECT:='127.0.0.1'} -r ts-node/register src/server.ts ts-node",
    "dev:watch": "nodemon",
    "build": "tsc",
    "server": "node build/src/server.js",
    "prestart": "npm run build",
    "start": "npm run server",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "tests/**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  },
