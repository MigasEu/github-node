{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/MigasEu/github-node.git"
  },
  "main": "build/server.js",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "typescript": "^3.4.4",
    "mocha": "^6.1.4",
    "chai": "^4.2.0",
    "@types/mocha": "^5.2.6",
    "@types/chai": "^4.1.7",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "@types/node": "^11.13.5",
    "@types/express": "^4.16.1",
    "nodemon": "^1.18.11",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0"
  },
  "scripts": {
    "dev": "node --inspect=${NODE_INSPECT:='127.0.0.1'} -r ts-node/register src/server.ts ts-node",
    "dev:watch": "nodemon",
    "build": "tsc",
    "server": "node build/server.js",
    "prestart": "npm run build",
    "start": "npm run server",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  },
  "author": "",
  "license": "ISC"
}
